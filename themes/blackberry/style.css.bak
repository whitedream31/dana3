* {
	margin: 0;
	padding: 0;
}

body {
	background: #FFFFFF url(images/img01.gif) repeat-x;
	text-align: left;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 13px;
	color: #333333;
}

form {
}

input, textarea {
	padding: 2px 5px;
	font: normal 1em "Trebuchet MS", Arial, Helvetica, sans-serif;
	color: #333333;
}

h1, h2, h3 {
	margin-top: 1.5em;
	font-weight: normal;
}

h1 {
	font-size: 2em;
}

h2 {
	font-size: 1.4em;
}

h3 {
	font-size: 1em;
}

p, ul, ol {
	margin-top: 1.5em;
	line-height: 160%;
}

ul, ol {
	margin-left: 3em;
}

blockquote {
	margin-left: 3em;
	margin-right: 3em;
}

a {
	color: #CC6B47;
}

a:hover {
	text-decoration: none;
	color: #3760BF;
}

a img {
	border: none;
}

img.left {
	float: left;
	margin: 0 20px 0 0;
}

img.right {
	float: right;
	margin: 0 0 0 20px;
}

hr {
	display: none;
}

/* Header */

#header {
	height: 110px;
	margin: 0 auto;
	background: #2E2E2E;
}

#logo {
	width: 778px;
	height: 110px;
	margin: 0 auto;
}

#logo h1, #logo h2 {
	margin: 0;
	letter-spacing: -0.05em;
	text-transform: lowercase;
	font-weight: normal;
	color: #FFFFFF;
}

#logo h1 {
	float: left;
	padding-top: 40px;
	font-size: 4em;
}

#logo h1 a {
	text-decoration: none;
	color: #FFFFFF;
}

#logo h2 {
	float: left;
	padding: 67px 0 0 8px;
	font-size: 2em;
	font-style: italic;
	color: #858585;
}

/* Menu */

#menu-wrap {
	background: #2E2E2E;
}

#menu {
	width: 778px;
	height: 50px;
	margin: 0 auto;
}

#menu ul {
	margin: 0;
	padding: 0;
	list-style: none;
	line-height: normal;
}

#menu li {
	display: inline;
}

#menu a {
	float: left;
	margin-right: 6px;
	padding: 5px 20px 5px 20px;
	background: #4C4D51;
	border: 1px #575C5F solid;
	text-decoration: none;
	text-transform: uppercase;
	font-weight: bold;
	font-size: 11px;
	color: #FFFFFF;
}

#menu a:hover {
	text-decoration: none;
	background: #983D3A;
	border: 1px #D45951 solid;
}

#menu .first a {
	background: #983D3A;
	border: 1px #D45951 solid;
}

/* Page */

#page {
	width: 778px;
	margin: 0 auto;
}

#content {
	background: #F4EDD3;
	border-bottom: 4px #BBB082 solid;
}

/* Post */

.post {
}

.post .title {
	margin: 0;
}

.post .title a {
	text-decoration: none;
}

.post .byline {
	margin: 0 0 30px 0;
	line-height: normal;
}

.post .entry {
}

.post .meta {
	clear: both;
}

.post .meta a {
	text-decoration: none;
	font-weight: bold;
}

.post .meta a:hover {
	text-decoration: underline;
}

.post .meta .comments {
	padding-left: 20px;
	background: url(images/img07.gif) no-repeat left center;
}

.post .meta .permalink {
	padding-left: 20px;
	background: url(images/img08.gif) no-repeat left center;
}

/* Latest Post */


#latest-post-wrap {
	background: #E8DBA7;
	border-bottom: 4px #BBB082 solid;
}

#latest-post {
	width: 778px;
	margin: 0 auto;
	padding: 0 0 20px 0;
}

#latest-post h1 {
	margin: 10px 0;
	padding: 0;
}

#latest-post p {
	margin: 0;
	padding-bottom: 15px;
}

#latest-post .meta {
	padding: 10px 0 0 0;
}

#latest-post .byline {
	margin: 0 0 10px 0;
	padding: 10px 0;
	background: #BBB082;
	font-size: .9em;
	font-weight: bold;
	color: #FFFFFF;
}

#latest-post .byline span {
	padding: 0 0 0 15px;
}

#latest-post .byline a {
	padding: 0 30px 0 0;
	background: url(images/img05.gif) no-repeat right top;
	text-decoration: none;
}

#latest-post .byline a:hover {
	text-decoration: underline;
}

/* Recent Posts */

#recent-posts {
	width: 778px;
	margin: 0 auto;
}

#recent-posts h2 {
	margin: 0;
	padding: 10px 0 10px 15px;
	background: #BBB082;
	letter-spacing: .5em;
	text-transform: uppercase;
	text-align: center;
	font-size: .9em;
	font-weight: bold;
	color: #FFFFFF;
}

#recent-posts h3 {
	margin: 20px 0 0 0;
	padding: 0;
	font-size: 1.4em;
}

#recent-posts h3 a {
	text-decoration: none;
}

#recent-posts ul {
	margin: 0;
	padding: 0;
	list-style: none;
}

#recent-posts li {
	float: left;
	width: 240px;
	padding-left: 29px;
}

#recent-posts li.first {
	padding: 0;
}

#recent-posts .byline {
	margin: 0 0 10px 0;
	padding: 0;
	border-bottom: 1px #DDD7C0 solid;
	text-align: left;
	font-size: 10px;
	font-weight: bold;
}

#recent-posts .byline span {
}

#recent-posts .byline a {
	padding: 0 30px 0 0;
	background: url(images/img05.gif) no-repeat right top;
	text-decoration: none;
}

#recent-posts .byline a:hover {
	text-decoration: underline;
}

#recent-posts p {
	padding: 0;
	margin: 0;
}


/* Sidebar */

#sidebar {
	width: 778px;
	margin: 0 auto;
}

#sidebar ul {
	margin: 0;
	padding: 0;
	list-style: none;
}

#sidebar li {
	float: left;
	width: 259px;
}

#sidebar li ul {
	padding-left: 20px;
}

#sidebar li li {
	float: none;
	width: auto;
	padding: 3px 0;
	background: none;
}

#sidebar h2 {
	margin: 0 0 20px 0;
	padding: 10px 0 10px 16px;
	background: #BBB082;
	text-transform: uppercase;
	letter-spacing: .5em;
	font-size: .9em;
	font-weight: bold;
	color: #FFFFFF;
}

/* Search */

#search {
}

#search #s {
	width: 80%;
	margin-bottom: 10px;
}

/* Calendar */

#calendar {
}

#calendar table, #calendar caption {
	width: 100%;
	text-align: center;
}

#calendar caption {
	font-weight: bold;
}

#calendar a {
	font-weight: bold;
}

/* Footer */

#footer {
	clear: both;
	margin: 0 auto;
	padding: 20px 0;
	background: url(images/img11.gif) repeat-x;
	text-align: center;
}

#footer p {
	margin: 0;
	font-size: .8em;
	color: #666666;
}




/** CONTACT */

.contact {
  margin: 20px 20px 20px 20px;
}

.contact label {
  float: none;
  display: block;
  color: #000000;
  margin: 15px 5% 0 5%;
  font-size: 16px;
}

.contact .contactinput {
  width: 90%;
  margin: 5px 5% 0 5%;
  text-align: left;
  font-size: 16px;
  background: #FFFFFF;
  color: #000000;
  font-weight: normal;
}

.contact  input[type="submit"] {
  margin: 5px 5% 0 5%;
}

#guestbook {
  width: 90%;
}

#guestbook  h3 {
  font-weight: bold;
  margin-top: 30px;
  margin-bottom: 0px;
}

.contactinput {
  width: 90%;
  margin: 5px 5% 0 5%;
  text-align: left;
  font-size: 16px;
  background: #FFFFFF;
  color: #000000;
  font-weight: normal;
}

#productlist h3 {
  font-size: 18px;
  color: #9F9F9F;
  font-weight: bold;
  margin-top: 0px;
  margin-bottom: 0px;
  padding-left: 0px;
  padding-right: 0px;
  padding-top: 2px;
  padding-bottom: 2px
}

#productlist .productitem {
  width: 75px;
  vertical-align: top;
  border: 1px;
}

#logoimg img {
  display: block;
  margin: 0 auto 0 auto;
}

.addthis {
  margin: 10px 0px 0px 0px;
  background: none;
  vertical-align: middle;
}

#productlist {
  width: 90%;
  border: 1px;
}

#productlist img {
  margin: 0px 10px 10px 0px;
}

#productlist td {
  margin: 2px 5px 2px 10px;
}

#productlist td p {
  margin: 0;
  height: auto;
  line-height: 125%;
}

#productlist .price {
  width: 75px;
  text-align: right;
  color: #000000;
  margin: 0 20px 0 0;
}

#productlist .productitemspecialoffer {
  font-weight: bold;
  font-size: large;
  color: #FF0000;
}

.productitemsection {
  margin-top: 0px;
  margin-bottom: 0px;
  border-top: none;
  border-bottom: thin dotted #000000;
  margin-left: 0px;
  margin-right: 0px;
  padding-bottom: 10px
}

#productlistheader, #productlistfooter, #galleryfooter {
  width: 100%;
  font-weight: normal;
  font-size: 14px;
  background-color: #BCE8EC;
  color: #000000;
  margin: 2px 0px 5px 0px;
  padding: 2px 2px 2px 2px;
}

#productlistheader img, #productlistfooter img, #galleryfooter img {
  border: 0;
  margin: 1px;
}

#productlistheader p, #productlistfooter p, #galleryfooter p {
  margin: 0;
  padding: 0;
  line-height: normal;
  float: left;
}

#productlistheader div, #productlistfooter div {
  padding: 2px 0px 2px 0px;
  margin: 5px 0 5px 0;
}

#productlistheader ul, #productlistfooter ul, #galleryfooter ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

#productlistheader li, #productlistfooter li, #galleryfooter li {
  margin: 0;
  padding: 0 0 0 5px;
  display: inline;
  text-align: center;
}

#productlistheader li a, #productlistfooter li a, #galleryfooter li a {
  color: #999999;
  text-decoration: none;
  text-align: center;
}

.gallery {
  color: #000000;
  padding: 2px 5px 2px 5px;
  margin: 5px 0 5px 0;
}

.gallery table {
  width: 100%;
  border: 3px solid #9F9F9F;
}

.gallery ul {
  list-style: none;
  margin: 0;
  padding: 0;
  list-style-position: outside;
}

.gallery ul li {
  padding: 10px;
  margin: auto auto;
  display: block;
  float: left;
}

.gallery ul img {
  border: 3px solid #9F9F9F;
  border-width: 3px;
}

.gallery ul a:hover img {
  border: 3px solid #9F9F9F;
  border-width: 3px;
}

.gallery .text {
  margin: 0px 0 10px 0px;
  padding: 0;
  display: block;
  line-height: normal;
  text-align: left;
  height: auto;
  min-height: 80px;
}

.gallery .text p {
  line-height: normal;
}

.gallery td {
  display: block;
  float: left;
  width: 40%;
  margin: 10px 10px 0 10px;
}

.gallery .image {
  float: none;
  display: block;
  margin: 0px auto;
}

.gallery .image p {
  padding: 10px 0 0 0;
  margin: 0;
  line-height: normal;
  min-height: 50px;
  text-align: center;
  display: table-cell;
  vertical-align: bottom;
}

.gallery .gap {
  margin: 0 20px 0 20px;
}

.error, .response {
  margin: 20px 0 20px 0;
  font-size: 18px;
  font-weight: bold;
}

.error {
  color: red;
}

.response {
  color: #000000;
}

ul.fileitem {
  list-style: none;
}

.fileitem  a {
  text-decoration: none;
}

.fileitem  img {
  border: 0;
  margin-right: 10px;
}

ul li.fileitem {
  list-style-type: none;
  list-style-position: inside;
  margin-left: -10px;
  background: none;
}

.advert {
  width: 778px;
  margin: 0 auto;
  display: block;
  padding: 5px;
  background: #FFFFFF;
  color: #000000;
  float: none;
  clear: both;
}

.advert p {
  margin: 0;
  padding: 0;
}

.advert a {
  width: 778px;
  margin: 0;
  padding: 0;
  color: #000000;
  display: block;
  font-size: 14px;
  line-height: normal;
  text-decoration: none;
}

.advertheader, .advertfooter {
  background: #000000;
  color: #FFFFFF;
  font-size: 10px;
  text-align: center;
  margin: 0px;
  padding: 0px;
  line-height: normal;
}

/* slide show */

/* rotator in-page placement */
.rotator {
  border: 0;
  position: relative;
  margin: 0px;
  padding: 0;
  display: none;
  list-style-type: none;
  list-style-image: none;
  list-style-position: inside;
}
/* rotator css */
.rotator ul {
  display: block;
  list-style-type: none;
  list-style-image: none;
  list-style-position: inside;
}
.rotator ul li {
  float: left;
  position: absolute;
  display: block;
  list-style-type: none;
  list-style-image: none;
  list-style-position: inside;
}
/* rotator image style */
.rotator ul li img {
  border: 1px solid #ccc;
  padding: 4px;
  background: #FFF;
}
.rotator ul li.show {
  z-index: 500
}
